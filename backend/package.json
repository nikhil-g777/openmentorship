{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=local nodemon app.js",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "pretty-quick --staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-path .gitignore .",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "ejs": "~2.5.7",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.2.13",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "query-string": "^6.12.0",
    "twilio": "^3.68.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^29.7.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.1.0",
    "supertest": "^6.3.3"
  }
}
